{
  "name": "insights-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development rspack server",
    "build": "cross-env NODE_ENV=production rspack build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "npm run lint --fix",
    "translations:build": "unified-translations --translations-file ./src/locales/translations.json --languages pt-br=./src/locales/pt_br.json,en=./src/locales/en.json,es=./src/locales/es.json",
    "translations:suggest-from-pt-br": "unified-translations --translations-file ./src/locales/translations.json --languages pt-br=./src/locales/pt_br.json,en=./src/locales/en.json,es=./src/locales/es.json --mode build --suggest \"pt-br:pt>en:en\" && unified-translations --translations-file ./src/locales/translations.json --languages pt-br=./src/locales/pt_br.json,en=./src/locales/en.json,es=./src/locales/es.json --mode reverse && unified-translations --translations-file ./src/locales/translations.json --languages pt-br=./src/locales/pt_br.json,en=./src/locales/en.json,es=./src/locales/es.json --mode build --suggest \"pt-br:pt>es\" && unified-translations --translations-file ./src/locales/translations.json --languages pt-br=./src/locales/pt_br.json,en=./src/locales/en.json,es=./src/locales/es.json --mode reverse"
  },
  "dependencies": {
    "@rspack/cli": "^1.1.5",
    "@rspack/core": "^1.1.5",
    "dotenv": "^16.4.7",
    "html-rspack-plugin": "^6.0.2",
    "sass-loader": "^16.0.4",
    "vue-loader": "^17.4.2",
    "cross-env": "^7.0.3",
    "@sentry/vue": "8.50.0",
    "@vueuse/core": "^10.11.0",
    "@weni/unnnic-system": "^2.25.0",
    "axios": "^1.7.2",
    "chart.js": "^4.4.2",
    "chartjs-plugin-datalabels": "^2.2.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.1.7",
    "emoji-mart": "^5.6.0",
    "firebase": "^10.13.1",
    "marked": "^14.1.2",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "vue": "^3.3.11",
    "vue-i18n": "9",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-istanbul": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.6",
    "@weni/eslint-config": "^1.0.3",
    "canvas": "^2.10.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.1.1",
    "node-sass": "^9.0.0",
    "prettier": "^3.2.5",
    "sass": "^1.69.7",
    "typescript": "^5.4.5",
    "vite": "^5.0.10",
    "vitest": "^2.0.5",
    "vue-eslint-parser": "^9.4.2"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
}